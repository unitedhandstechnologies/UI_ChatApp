                    vid.volume = 1.0;
                });
                $("#payvideo").on('click', function () {
                    $("#video_div").show();
                    $("#something").hide();

                    if (!is_play) {
                        vid.play();
                        is_play = true;
                        playtext = 'Pause Video';
                    } else {
                        vid.pause();
                        playtext = 'Play Video';
                        is_play = false;
                    }
                    $("#payvideo").text(playtext);
                });
            });
